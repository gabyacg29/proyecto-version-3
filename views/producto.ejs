<div class="col-12 col-sm-6 col-lg-3">
    <section class="product-box">
        <a href="<%=data[i].Id%>">
            <figure class="product-box_image">
                <img src="<%= data[i].Imagen %>" alt="imagen de producto">
            </figure>
            <article class="product-box_data">
                <h2><%= data[i].Precio %></h2>
                <p><%= data[i].Nombre %></p>
                <i class="fas fa-cart-plus"></i>
            </article>
        </a>
    </section>
</div>