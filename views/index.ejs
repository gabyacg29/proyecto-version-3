<!-- Incluye Head y Header -->
<%- include('./partials/head.ejs')%> 
<%- include('./partials/header.ejs')%>

<!-- Carrusel -->
<div class="bd-example">
    <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="carousel-item active style=100vh">
                <img class="d-block w-100 banner"
                    data-src="holder.js/700x350?auto=yes&amp;bg=777&amp;fg=555&amp;text=First slide"
                    alt="First slide [900x300]"
                    src="../public/img/vinos1.jpg"
                    data-holder-rendered="true">
                <div class="carousel-caption d-none d-md-block">
                   
                </div>
            </div>
            <div class="carousel-item">
                <img class="d-block w-100 banner"
                    data-src="holder.js/700x350?auto=yes&amp;bg=666&amp;fg=444&amp;text=Second slide"
                    alt="Second slide [900x300]"
                    src="../public/img/vinos2.jpg"
                    data-holder-rendered="true">
                <div class="carousel-caption d-none d-md-block">
                    
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>




<!-- Productos -->
<div class="container products-wrapper">
  <div class="row">
    <div class="col-12">
      <h2 class="products-title">Vinos</h2>
    </div>
    <!-- Bucle sistema recorre array de objetos que envia el controlador y para cada obejto levanta un template de producto -->
            <% for( var i=0; i<(productos.length); i++) { %>
                <%- include('./producto.ejs')  %>
            <% } %>


  </div>
</div>
<!-- Incluye Footer -->
<%- include('./partials/footer.ejs') %>
